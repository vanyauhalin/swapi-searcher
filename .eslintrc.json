{
  "extends": "@vanyauhalin/eslint-config-react",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "overrides": [
    {
      "files": "vite.config.js",
      "rules": {
        "import/no-default-export": "off"
      }
    },
    {
      "files": "*.tsx",
      "rules": {
        "unicorn/no-null": "warn"
      }
    }
  ]
}
